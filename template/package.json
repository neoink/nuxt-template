    "@nuxtjs/axios": "^5.0.1",
    {{#if deviceDetect}}
    "nuxt-device-detect": "^1.1.0",
    {{/if}}
    "cross-env": "^5.0.1",
    "dotenv": "^5.0.1",
    "express": "^4.15.3",
    "express-session": "^1.15.6",
    "helmet": "^3.12.1",
    "cors": "^2.8.4",
    "nuxt": "latest",
    "source-map-support": "^0.4.15",
    "body-parser": "^1.18.2",    
    "backpack-core": "^0.5.0",
    "css-mqpacker": "^6.0.2",
    "postcss-nested": "^3.0.0",
    "uuid": "^3.2.1",
